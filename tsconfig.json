        "./dist/tango-app-account-settings"
      ],
      "tango-app-auth": [
        "./dist/tango-app-auth"
      ],
      "tango-app-metronics": [
        "./dist/tango-app-metronics"
      ],
      "tango-app-shared": [
        "./dist/tango-app-shared"
      ],
      "tango-auth": [
        "./dist/tango-auth"
      ],
      "tango-manage": [
        "./dist/tango-manage"
      ],
      "tango-manage-audit": [
        "./dist/tango-manage-audit"
      ],
      "tango-manage-brands": [
        "./dist/tango-manage-brands",
        "./dist/tango-manage-brands"
      ],
      "tango-manage-overview": [
        "./dist/tango-manage-overview"
      ],
      "tango-manage-reports": [
        "./dist/tango-manage-reports"
      ],
      "tango-manage-settings": [
        "./dist/tango-manage-settings"
      ],
      "tango-manage-stores": [
        "./dist/tango-manage-stores"
      ],
      "tango-manage-tickets": [
        "./dist/tango-manage-tickets"
